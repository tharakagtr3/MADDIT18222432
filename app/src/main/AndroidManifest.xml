<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yashodhara.joyJourney">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yashodhara.joyJourney.UploadImageActivity"></activity>
        <activity android:name="com.yashodhara.joyJourney.SigninActivity" />
        <activity android:name="com.yashodhara.joyJourney.HomeActivity" />
        <activity android:name="com.yashodhara.joyJourney.BudgetActivity" />
        <activity android:name="com.yashodhara.joyJourney.TransportationActivity" />
        <activity android:name="com.yashodhara.joyJourney.DurationActivity" />
        <activity android:name="com.yashodhara.joyJourney.ObjectivesActivity" />
        <activity android:name="com.yashodhara.joyJourney.DetailsActivity" />
        <activity android:name="com.yashodhara.joyJourney.UploadActivity" />
        <activity
            android:name="com.yashodhara.joyJourney.RegisterActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>